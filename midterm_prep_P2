import re

dna_seq = ""

def count_bases(s):
    from collections import Counter
    return dict(Counter(s))


def bio_to_regex(pattern_bio):
    # A handy conversion table, to map bio letters to regex subpatterns:
    bio_map = {'R': '[AG]', 'Y': '[TC]', 'K': '[GT]', 'M': '[AC]', 'S': '[GC]', 'W': '[AT]',
                         'B': '[^A]', 'D': '[^C]', 'H': '[^G]', 'V': '[^T]', 'N': '.'}
    
    return ''.join([bio_map.get(letter,letter) for letter in pattern_bio])


def sim_cuts(site_pattern, s):
    regex_pre = bio_to_regex(site_pattern)
    first, second = regex_pre.split('|')
    pattern = "(" + first + ")(" + second + ")"
    new = re.sub(r"" + pattern, r'\1|\2', s)
    return new.split("|")

print(sim_cuts('ANT|AAT', 'GCATAGTAATGTATTAATGGC'))